c := 'A'
while <(c, +('Z', '\x01')):
    if <(c, 'G'):
        print("-")
    elif <(c, 'M'):
        print("+")
    elif <(c, 'T'):
        print("*")
    elif ==(c, 'W'):
        print("w")
    elif ==(c, 'Z'):
        print("!")
    else:
        print(repr(c))
    c := +(c, '\x01')

