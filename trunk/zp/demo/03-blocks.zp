# while and if blocks.

c 'A'
while <(c, +('Z', 0x01))
    print(arr(c))
    c +(c, 0x01)

print("\n")

c 'A'
while <(c, +('Z', 0x01))
    if <(c, 'G')
        print("-")
    elif <(c, 'M')
        print("+")
    elif <(c, 'T')
        print("*")
    elif ==(c, 'W')
        print("w")
    elif ==(c, 'Z')
        print("!")
    else
        print(arr(c))
    c +(c, 0x01)

print("\n")
